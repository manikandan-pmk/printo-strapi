{
  "kind": "collectionType",
  "collectionName": "logins",
  "info": {
    "singularName": "login",
    "pluralName": "logins",
    "displayName": "Login"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Username": {
      "type": "string"
    },
    "Email": {
      "type": "email"
    },
    "Password": {
      "type": "password"
    },
    "jwt": {
      "type": "text"
    },
    "carts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::cart.cart",
      "mappedBy": "login"
    },
    "payments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::payment.payment",
      "mappedBy": "login"
    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "login"
    }
  }
}
