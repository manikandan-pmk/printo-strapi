{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Price": {
      "type": "biginteger"
    },
    "Quantity": {
      "type": "integer"
    },
    "Image": {
      "type": "string"
    },
    "Condition": {
      "type": "enumeration",
      "enum": [
        "paid",
        "pending"
      ]
    },
    "login": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::login.login",
      "inversedBy": "orders"
    },
    "payment": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::payment.payment",
      "inversedBy": "orders"
    }
  }
}
